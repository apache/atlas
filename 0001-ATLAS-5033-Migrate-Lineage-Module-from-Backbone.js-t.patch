+            control={
+              <Checkbox
+                checked={filters.showTooltip}
+                onChange={handleCheckboxChange}
+                name="showTooltip"
+              />
+            }
+            label="Show node details on hover"
+          />
+          <FormControlLabel
+            control={
+              <Checkbox
+                checked={filters.labelFullName}
+                onChange={handleCheckboxChange}
+                name="labelFullName"
+              />
+            }
+            label="Display full name"
+          />
+        </Stack>
+      </Stack>
+    </>
+  );
+};
